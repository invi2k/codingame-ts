{
  "name": "codingame-ts",
  "description": "CodinGame Typescript Root Project",
  "version": "1.0.0",
  "scripts": {
    "build": "webpack --mode development",
    "test": "ts-node node_modules/jasmine/bin/jasmine --config=jasmine.json",
    "coverage": "nyc -r text -e .ts -x \"src/test/*.test.ts\" npm run test",
