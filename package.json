    "pretest": "npm.cmd run compile",
    "posttest": "npm.cmd run check"
  },
  "author": "Torben Krupski",
  "repository": {
    "type": "git",
    "url": "https://github.com/jbnimble/codingame-typescript.git"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jasmine": "^3.5.10",
    "awesome-typescript-loader": "^4.0.1",
    "jasmine": "^3.5.0",
    "nyc": "^15.0.1",
    "ts-node": "^8.10.1",
    "typescript": "^3.6.3",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.3.11",
    "gts": "^2.0.2",
    "@types/node": "^13.11.1"
  }
}
